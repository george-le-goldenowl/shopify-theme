<div class="blog__mv">
  {% if section.settings.banner != blank %}
    <img src="./assets/images/blog.png" alt="">
  {% else %}
    {{ 'lifestyle-1' | placeholder_svg_tag }}
  {% endif %}
</div>
{% schema %}
  {
    "name": "Blogs Banner",
    "settings": [
      {
        "id": "banner",
        "type": "image_picker",
        "label": "Desktop banner"
      }
    ]
  }
{% endschema %}